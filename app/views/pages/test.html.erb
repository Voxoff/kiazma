<% content_for :title do %>
  Kitchup
<% end %>
<div style="background: linear-gradient( rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.5) ), url('image_path('homepage-banner-real.jpg')'); background-position: 50% 50%" class="banner">
  <div class="banner-content">
    <div class="mobile-header">
      <h1 class="mb-4">The place for kitchen space</h1>
    </div>
    <div class="desktop-header d-flex justify-content">
      <div class="media-test">
        <h1 class="neg-btm-marg">The place for  </h1>
        <div class="type-box">
          <h1 class="mb-4" id="banner-typed-text"></h1>
        </div>
      </div>
    </div>
    <!-- On all screens except mobile -->
    <div class="search-bar d-flex">
      <% if false %>
      <%= simple_form_for :kitchen, url: kitchens_path, html: { class: "d-flex searchform form-control-group", id: "homesearch"}, method: :get do |f| %>
        <%= f.input :search, label: false, wrapper: false, input_html: { class: "form-control-searchbar mt-2" }, type: "text", placeholder: "Where are you looking? E.g. Battersea"%>
        <button type="submit" class="search-button"><i class="fas fa-search"></i><span class="mob-hide">  Search</span></button>
      <% end %>
      <% end %>
      <script>
        document.getElementById('kitchen_search').onkeydown = function(e){
         if(e.keyCode == 13){
           document.getElementById("homesearch").submit()
         }
      };
      </script>

    </div>
  </div>
  <% if false %>
  <%= link_to(@main_kitchen) do %>
    <div class="featured-kitch-box orange">
      <% unless @main_kitchen.nil? %>
      <h5><%= @main_kitchen.name %></h5>
      <% if @main_kitchen.price_per_day %>
        <p class="mb-1">£<%= @main_kitchen.price_per_day %> / day</p>
      <% elsif @main_kitchen.price_per_week %>
        <p class="mb-1">£<%= @main_kitchen.price_per_week %> / week</p>
      <% elsif @main_kitchen.price_per_month %>
        <p class="mb-1">£<%= @main_kitchen.price_per_month %> / month</p>
      <% end %>
      <% end %>
    </div>
  <% end %>
  <% end %>
</div>
